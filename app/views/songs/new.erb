<h1>Create a new song here</h1>

<form action="/songs" method="POST">
  <label>Name:</label>
  <br>
  <input type="text" name="song[name]" id="Name">
  <br>
  <label>What genre is this song?</label>
  <%@genres.each do |genre|%>
  <input type="checkbox" name="song[genre]" id="<%=genre.name%>"><%=genre.name%></input>
  <%end%>

  <input type="text" name="song[artist]" id="Artist Name"></input>

 <br>
  <input type="submit" value="Create Artist">
  </form>
